        }
    },
    "autoload-dev": {
        "psr-4": {
            "A969350794\\LaravelSearchKit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "A969350794\\LaravelSearchKit\\LaravelSearchKitServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/your-repo/issues",
        "source": "https://github.com/your-repo/laravel-search-kit"
    }
